<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <!-- 是否开启自动驼峰命名规则（camel case）映射，即从数据库列名 A_COLUMN 到属性名 aColumn 的类似映射 -->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
        <typeAlias alias="user" type="com.example.house.entity.User"/>
        <typeAlias alias="house" type="com.example.house.entity.House"/>
        <typeAlias alias="houseUser" type="com.example.house.entity.HouseUser"/>
        <typeAlias alias="orders" type="com.example.house.entity.Orders"/>
        <typeAlias alias="blog" type="com.example.house.entity.Blog"/>
    </typeAliases>
</configuration>
